@model MultasAdmin.Models.Policia

@{
    ViewData["Title"] = "Policia Details";
}

<h1>Policia Details</h1>

<div>
    <h4>ID:</h4>
    <p>@Model.ID</p>
</div>
<div>
    <h4>Rec:</h4>
    <p>@Model.Rec</p>
</div>
<div>
    <h4>Nombre:</h4>
    <p>@Model.Nombre</p>
</div>
<div>
    <h4>Placa:</h4>
    <p>@Model.Placa</p>
</div>
<div>
    <h4>Supervisor:</h4>
    <p>@Model.Supervisor</p>
</div>
<div>
    <h4>Activo:</h4>
    <p>@Model.Activo</p>
</div>
<div>
    <h4>Unidad:</h4>
    <p>@Model.Unidad</p>
</div>

<div class="form-group">
    <a asp-action="Edit" asp-route-id="@Model.ID" class="btn btn-primary">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>
