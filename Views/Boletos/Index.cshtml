@model IEnumerable<MultasAdmin.Models.Boleto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Rec</th>
            <th>Num Boleto</th>
            <th>Device Name</th>
            <th>Fecha</th>
            <th>Hora</th>
            <th>Municipio</th>
            <th>Tablilla</th>
            <th>CMag</th>
            <th>Cap</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Rec</td>
                <td>@item.NumBoleto</td>
                <td>@item.DeviceName</td>
                <td>@item.Fecha</td>
                <td>@item.Hora</td>
                <td>@item.Municipio</td>
                <td>@item.Tablilla</td>
                <td>@item.CMag</td>
                <td>@item.Cap</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
